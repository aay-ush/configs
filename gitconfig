alias.last=log --name-status -1
alias.lg=log --oneline --graph --all --decorate
alias.unstage=reset HEAD --
alias.st=status
alias.co=checkout
alias.cob=checkout -b
alias.cm=commit -m
alias.cma=commit -am
alias.cam=commit --amend -m
alias.camc=commit --amend -C
alias.amend=commit --amend --no-edit
alias.rb=rebase
alias.rbc=rebase --continue
alias.rba=rebase --abort
alias.cp=cherry-pick
alias.cpc=cherry-pick --continue
alias.cpa=cherry-pick --abort
alias.df=difftool -y
alias.fulldiff=difftool -dy
